<application>
	<!-- yarpmanager file for the object surface exploration experiments -->
	<name> ObjectSurfaceExploration </name>
	<description> Object surface exploration 	</description>
	<version> 0.0 </version>

	<authors>
		<author email="nawid.jamali@iit.it"> Nawid Jamali </author>
	</authors>

	<!-- Required modules -->
	<module>
		<name>object-surface-exploration</name>
		<parameters>--context object-surface-exploration</parameters>
		<node>localhost</node>
		<display></display>
		<stdio></stdio>
	</module>

	<module>
	  <name>iCub_SIM</name>
	  <node>localhost</node>
	</module>

	<module>
	  <name>simCartesianControl</name>
	  <parameters>--no_legs</parameters>
	  <node>localhost</node>
	</module>

	<module>
	  <name>iKinCartesianSolver</name>
	  <parameters>--context simCartesianControl --part left_arm</parameters>
	  <node>localhost</node>
	</module>
  
  
  
	<!--<connection> 
		<from>/cam/right</from> 
		<to>/view/right</to> 
		<protocol>udp</protocol> 
	</connection> -->

</application>
